    .products-vertical-slider .slick-prev {
        left: 50%;
        top: 0px;
        width: 15px;
        height: 15px;
        transform: translateX(-50%);
    }

    .products-vertical-slider .slick-next {
        left: 50%;
        top: auto;
        width: 15px;
        height: 15px;
        right: auto;
        bottom: 0px;
        transform: translateX(-50%);
    }

    .products-vertical-slider .product-info  {
        position: absolute;
        right: 100%;
        visibility: hidden;
        opacity: 0;
        margin-top: 0px;
        display: flex;
        z-index: -1;
        padding: 0px;
        pointer-events: none;
        transition: visibility 0s ease 0s, opacity 0.25s linear 0s;
