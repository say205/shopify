@font-face {
    font-family: 'Island Style';
    src: url('IslandStyle.otf') format("opentype");
    font-style: normal;
}

$font-custom: 'Island Style';

{% assign header_font = settings.type_header_font %}
{% assign base_font = settings.type_base_font %}
{% assign heading_font = settings.type_heading_font %}

{{ header_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
{{ base_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}
{{ heading_font | font_face | replace: '@font-face {', '@font-face { font-display: swap;' }}

@function em($pxval, $base: $font-size-base) {
    @if not unitless($pxval) {
        $pxval: strip-units($pxval);
    }
    @if not unitless($base) {
        $base: strip-units($base);
    }
    @return ($pxval / $base) * 1em;
}

@function strip-units($value) {
    @return ($value / ($value * 0 + 1));
}

